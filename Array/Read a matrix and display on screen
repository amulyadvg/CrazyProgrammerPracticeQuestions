#include <iostream>
#include<math.h>

using namespace std;

int main() {
  int a[20][20];
  int m, n;
 
  cout << "Enter rows and columns of matrix A: " << endl;
  cin >> m >> n;
  
  cout << "Enter matrix A elements: " << endl;
  for (int i=0; i < m; i++) {
    for (int j=0; j < n; j++) {
      cin >> a[i][j];
    }
  }

  cout << "Matrix A: " << endl;
  for (int i=0; i < m; i++) {
    for (int j=0; j < n; j++) {
      cout << a[i][j] << " ";
    }
    cout << endl;
  }
return 0;
}
