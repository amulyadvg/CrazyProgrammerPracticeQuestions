#include <iostream>
#include<math.h>

using namespace std;

int main() {
  int x, y;
  cin >> x >> y;
  int quo, rem;
  
  if (x > y) {
    quo = x / y;
    rem = x % y;
    cout << quo << " " << rem << endl;
  }
  
  else if (y > x) {
    quo = y / x;
    rem = y % x;
    cout << quo << " " << rem << endl;
  }
  
return 0;
}
