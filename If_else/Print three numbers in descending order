#include <iostream>
#include<math.h>

using namespace std;

void desOrder(int a, int b, int c) {
  
  if (a > b && a > c) {
    if (b > c) {
    cout << a << " " << b << " " << c << endl;
    } 
    else {
    cout << a << " " << c << " " << b << endl;
    }
  }
  
  if (b > a && b > c) {
    if (a > c) {
    cout << b << " " << a << " " << c << endl;
    }
    else {
    cout << b << " " << c << " " << a << endl;
    }
  }
  
  if (c > a && c > b) {
    if (a > b) {
      cout << c << " " << a << " " << b << endl;
    }
    else {
      cout << c << " " << b << " " << a << endl;
    }
  }
}

int main() {
  int x = 5;
  int y = 9;
  int z = 1;
  
  desOrder(x, y, z);
  
return 0;
}
