#include <iostream>
#include<math.h>

using namespace std;

int main() {
  int n, x;
  int large = 0;
  
  cout << "Enter number of elements: " << endl;
  cin >> n;
  
  for (int i=0; i < n; i++) {
    cout << "Enter number " << i+1 << ":" << endl;
    cin >> x;
    if (x > large) 
      large = x;
  }
  
  cout << "Largest number in array: " << large << endl;

return 0;
}
